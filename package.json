{
  "name": "co-mocha",
  "version": "1.1.0",
  "description": "Enable support for generators in Mocha tests",
  "main": "co-mocha.js",
  "scripts": {
    "lint": "standard",
    "test-spec": "mocha -R spec --bail",
    "test-cov": "node --harmony node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha -- -R spec --bail",
    "test": "npm run lint && npm run test-cov"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/blakeembrey/co-mocha.git"
  },
  "keywords": [
    "co",
    "mocha",
    "harmony",
    "generators"
  ],
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blakeembrey/co-mocha/issues"
  },
  "homepage": "https://github.com/blakeembrey/co-mocha",
  "devDependencies": {
    "bluebird": "^2.3.11",
    "istanbul": "git://github.com/gotwarlost/istanbul#harmony",
    "mocha": "^2.2.5",
    "pre-commit": "^1.0.7",
    "regenerator": "^0.8.26",
    "standard": "^3.11.1",
    "traceur": "0.0.89"
  },
  "dependencies": {
    "co": "^4.0.0",
    "is-generator": "^1.0.1"
  },
  "peerDependencies": {
    "mocha": ">=1.18 <3"
  }
}
